fm.base <- occu(~orario_occasione + MDR + temperatura_occasione + precipitazioni_occasione
~slope_degr + I(slope_degr^2) +
MDR + I(MDR^2) +
IQR + I(IQR^2) +
Esp.tr + I(Esp.tr^2) +
specie_dominante,
blgr)
fm.base
